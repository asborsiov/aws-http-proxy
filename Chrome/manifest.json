{
  "manifest_version": 2,
  "name": "Proxy",
  "version": "0.1.0",

  "permissions": ["<all_urls>", "background", "proxy", "storage", "alarms"],
  "background": {
  "scripts": ["background.js", "axios.min.js"],
    "persistent": true
   },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "blank.png"
  }
}